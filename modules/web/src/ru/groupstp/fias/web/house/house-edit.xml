<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="ru.groupstp.fias.web.house.HouseEdit"
        datasource="houseDs"
        focusComponent="fieldGroup"
        messagesPack="ru.groupstp.fias.web.house">
    <dsContext>
        <datasource id="houseDs"
                    class="ru.groupstp.fias.entity.House"
                    view="parent"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="houseDs">
            <column width="250px">
                <field property="eststatus"/>
                <field property="buildnum"/>
                <field property="strucnum"/>
                <field property="strstatus"/>
                <field property="startdate"/>
                <field property="enddate"/>
                <field property="parent"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
